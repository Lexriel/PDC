TARGETPATH = $(HOME)/public_html/C
DOC=Cours

doit: 
	latex $(DOC).tex
	latex $(DOC).tex
	dvips -t landscape $(DOC).dvi -o tmp.ps
	ps2pdf tmp.ps
	psnup -r -4 tmp.ps > $(DOC)$(REP).ps
	rm tmp.ps $(DOC).log $(DOC).dvi $(DOC).aux 
	mv $(DOC)$(REP).ps $(TARGETPATH)/.
	mv tmp.pdf $(TARGETPATH)/$(DOC)$(REP).pdf
